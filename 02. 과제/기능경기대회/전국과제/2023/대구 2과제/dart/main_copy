for i in range(4):
    if i == 3:
        ress.append(main(i,root[i],leaf[i]))
    else:
        ress.append(main(i,root[i]))
    tl(i,'done !!!')
    
for i in [0,2,3,1]:
    grip(0)  
    tool = 0 
    T,pos,res = i,poss[tool][i],ress[i]
    Run = [A,B,C,D][i]
    Run()
    
